{
  "expo": {
    "name": "JewGo",
    "slug": "jewgo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.neleoko.jewgo"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "jewgo",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "06dd70cb-984d-4692-905b-8833297bd588"
      }
    },
    "owner": "neleoko"
  }
}
